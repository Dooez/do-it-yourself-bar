      <default>false</default>
    </entry>
    <entry name="BlockEColorOverride" type="Bool">
      <default>false</default>
    </entry>
    <entry name="BlockEColorSource" type="Int">
      <default>0</default>
    </entry>
    <entry name="BlockEColor" type="String">
      <default></default>
    </entry>

    <!-- Appearance - Block indicator F-->
    <entry name="BlockFShapeOverride" type="Bool">
      <default>false</default>
    </entry>
    <entry name="BlockFShape" type="Int">
      <default>0</default>
    </entry>
    <entry name="BlockFRadius" type="Int">
      <default>2</default>
    </entry>
    <entry name="BlockFInvertPosition" type="Bool">
      <default>false</default>
    </entry>
    <entry name="BlockFColorOverride" type="Bool">
      <default>false</default>
    </entry>
    <entry name="BlockFColorSource" type="Int">
      <default>0</default>
    </entry>
    <entry name="BlockFColor" type="String">
      <default></default>
    </entry>
  </group>
</kcfg>
